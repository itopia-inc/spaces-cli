{
  "name": "spaces",
  "description": "A CLI wrapper for the itopia Spaces API",
  "version": "0.0.0",
  "author": "Jan Van Bruggen @JanCVanB",
  "bin": {
    "spaces": "./bin/run"
  },
  "bugs": "https://github.com/itopia/spaces-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "graphql": "^15.5.3",
    "graphql-request": "^3.5.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/near-operation-file-preset": "^2.1.3",
    "@graphql-codegen/typescript": "2.2.1",
    "@graphql-codegen/typescript-oclif": "^2.1.3",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "pkg": "^5.3.1",
    "prettier": "2.3.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/itopia/spaces-cli",
  "keywords": [
    "oclif"
  ],
  "license": "none",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "main": "lib/index.js",
  "oclif": {
    "bin": "spaces",
    "commands": "./lib/commands",
    "description": "A CLI wrapper for the itopia Spaces API\nFor more information about itopia Spaces, see https://spaces.itopia.com\nFor more information about the itopia Spaces API, see https://api.spaces.itopia.com\n\nExamples:\nspaces organization:create --name='My Organization'",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "organization": {
        "description": "Manage your organization(s)"
      }
    }
  },
  "repository": "itopia/spaces-cli",
  "scripts": {
    "build": "pkg . --out-path=dist",
    "codegen": "graphql-codegen --config codegen.yml",
    "docsgen": "oclif-dev readme",
    "format": "prettier --write .",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prebuild": "npm run codegen",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "prepare": "husky install",
    "pretest": "npm run codegen",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
